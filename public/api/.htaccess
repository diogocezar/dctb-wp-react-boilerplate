
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /dctb-wp-react-boilerplate/public/api/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /dctb-wp-react-boilerplate/public/api/index.php [L]
</IfModule>
# END WordPress